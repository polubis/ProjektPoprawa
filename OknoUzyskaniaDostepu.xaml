<Window x:Class="Projekt_Poprawa.OknoUzyskaniaDostepu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="OknoUzyskaniaDostepu" Height="280.995" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <WrapPanel>
            <Label Height="26" Width="192" Margin="60,5,0,0" FontWeight="Bold" Content="           Wpisz swój login "/>
            <TextBox Width="200" Height="30" Margin="45,5,0,0" x:Name="pobierzLogin"></TextBox>
            <Label Height="26" Width="192" Margin="60,5,0,0" FontWeight="Bold" Content="Wpisz swój kod zabezpieczeń"/>
            <PasswordBox Width="200" Height="30" Margin="45,5,0,0" x:Name="pobierzKod"></PasswordBox>
            <Button Width="101" Height="37" Margin="30,10,0,0" Click="Potwierdz" FontWeight="Bold" Cursor="Hand">Potwierdź</Button>
            <Button Width="101" Height="37" Margin="30,10,0,0" Click="Anuluj" FontWeight="Bold" Cursor="Hand">Anuluj</Button>
        </WrapPanel>
    </Grid>
</Window>
